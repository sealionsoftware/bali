import bali.Console
import bali.Boolean
import bali.String
import bali.Executable
import bali.Exception
import bali.RuntimeException

class TryDoer() implements Executable {

 	method execute(){

		try {
			throw new RuntimeException("Test")
		} catch (Exception e){
            throw new RuntimeException(e.getMessage() + " 2")
        } catch (Exception e){
            CONSOLE << e.getMessage()
        }

        throw new RuntimeException("Uncaught")
 	}

}
