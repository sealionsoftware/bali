import bali.Console;
import bali.Number;
import bali.Executable;

class WhileDoer() implements Executable {

 	method execute(){

		Number flag = 0;

	    while (true){
	        CONSOLE << "Hello";
	        if (flag == 3){
	            break;
	        }
	        flag = flag + 1;
	    }
 	}

}
