import bali.Console;
import bali.Number;
import bali.Executable;

class WhileDoer() implements Executable {

 	method execute(){

		Number flag = 0;

	    while (flag.lessThan(4)){
	        CONSOLE.writeLine("Hello");
	        if (flag.greaterThan(1)){
	            break;
	        }
	        flag = flag.add(1);
	    }
 	}

}
