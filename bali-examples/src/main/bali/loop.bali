import bali.Console;
import bali.Number;
import bali.Executable;
import bali.Value;

class WhileDoer() implements Executable {

 	method execute(){

		Number i = 0;

	    while (true){
	        CONSOLE << ("Hello " + NUMBER_FACTORY.format(i));
	        if (i == 3){
	            break;
	        }
	        i = i + 1;
	    }
 	}
}
