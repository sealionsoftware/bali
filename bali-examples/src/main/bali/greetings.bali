import bali.Console;
import bali.String;

constant String GREETING = "Constant Greeting";

interface Greeter {
 	declare greet();
}

class ConsoleGreeter() implements Greeter {

	field String greeting = "Field Greeting";

 	method greet(){
	    String greetingTwo = "Variable Greeting";
	    greetingTwo = GREETING;
		CONSOLE.printLine(greetingTwo);
 	}

}
