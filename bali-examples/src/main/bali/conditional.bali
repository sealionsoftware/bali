import bali.Console;
import bali.Boolean;

interface Doer {
 	declare goDo();
}

class ConditionalDoer() implements Doer {

 	method goDo(){

 	    Boolean one = false;
 	    Boolean two = true;

	    if (one){
	        CONSOLE.writeLine("First Block");
	    } else if (two){
	        CONSOLE.writeLine("Second Block");
	    } else {
	        CONSOLE.writeLine("Third Block");
	    }
 	}

}
