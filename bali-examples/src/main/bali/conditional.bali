import bali.Console;
import bali.Boolean;

interface Doer {
 	declare goDo();
}

class ConditionalDoer() implements Doer {

 	method goDo(){

 	    Boolean one = false;
 	    Boolean two = true;

	    if (one){
	        CONSOLE << "First Block";
	    } else if (two){
	        CONSOLE << "Second Block";
	    } else {
	        CONSOLE << "Third Block";
	    }
 	}

}
