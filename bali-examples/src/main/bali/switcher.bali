import bali.Console
import bali.Number
import bali.String
import bali.Executable

object SwitchExecutable implements Executable {

	field Number operand = 1

 	method execute {

		switch (operand){
			case 0: {
				CONSOLE << "Operand was 0";
			}
			case 1: {
            	CONSOLE << "Operand was 1";
            }
			default: {
				CONSOLE << "?";
			}
		}

 	}

}
