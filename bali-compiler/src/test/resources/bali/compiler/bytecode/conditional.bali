import bali.Console
import bali.Boolean

interface Doer {
 	declare goDo
}

object ConditionalDoer implements Doer {

 	method goDo {

 	    Boolean one = false;
 	    Boolean two = false;

	    if (one){
	        CONSOLE.printLine("First Block");
	    } else if (two){
	        CONSOLE.printLine("Second Block");
	    } else {
	        CONSOLE.printLine("Third Block");
	    }
 	}

}
